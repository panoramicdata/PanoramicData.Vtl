<Project Sdk="Microsoft.NET.Sdk">

<PropertyGroup>
	<TargetFramework>net10.0</TargetFramework>
	<Nullable>enable</Nullable>
	<LangVersion>latest</LangVersion>
	<ApplicationIcon>Icon.ico</ApplicationIcon>
	<Authors>Panoramic Data Limited</Authors>
	<PackageProjectUrl>https://github.com/panoramicdata/PanoramicData.Vtl/</PackageProjectUrl>
	<Copyright>Copyright Â© Panoramic Data Limited 2025</Copyright>
	<PackageLicenseUrl></PackageLicenseUrl>
	<PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
	<PackageIcon>Icon.png</PackageIcon>
	<PackageTags>VTL Library</PackageTags>
	<Title>PanoramicData.Vtl</Title>
	<Description>A .NET library for the Velocity Template Language (VTL)</Description>
	<Summary>A .NET library for the Velocity Template Language (VTL)</Summary>
	<Company>Panoramic Data Limited</Company>
	<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	<LangVersion>latest</LangVersion>
	<RepositoryType>git</RepositoryType>
	<RepositoryUrl>https://github.com/panoramicdata/PanoramicData.Vtl/</RepositoryUrl>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<IncludeSymbols>true</IncludeSymbols>
	<SymbolPackageFormat>snupkg</SymbolPackageFormat>

	<PackageReleaseNotes>Moved to net10.0</PackageReleaseNotes>
	<TreatWarningsAsErrors>true</TreatWarningsAsErrors>

</PropertyGroup>

<ItemGroup>
  <PackageReference Include="PanoramicData.NCalcExtensions" Version="5.8.17" />
</ItemGroup>
	<ItemGroup>
		<None Include="Icon.png" Pack="true" PackagePath="" />
	</ItemGroup>
	<ItemGroup>
	  <PackageReference Update="Nerdbank.GitVersioning" Version="3.9.50" />
	</ItemGroup>
</Project>
